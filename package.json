{
    "scripts": {
        "build": "npm run build-server",
        "build-server": "cd backend && npm install",
        "build-client": "cd Frontend/client && npm install",
        "start-dev": "npm run start-server-dev",
        "start-server-dev": "cd backend && npm run start",
        "start-client-dev": "cd Frontend/client && npm run start",
        "start-prod": "npm run start-server-prod",
        "start-server-prod": "cd backend && npm run start",
        "start-client-prod": "cd Frontend/client && npm run start"
    },
    "engines": {
        "node": "^8.5.5"
    },
    "dependencies": {
        "react-scripts": "^5.0.1"
    }
}
